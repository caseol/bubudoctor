<div class="container-fluid">
  <div class="row">
    <div class="col-sm-12 sectionform">
      <h3>Usuários do Sistema</h3>
      <table class="table table-hover responsive nowrap dttb">
        <thead>
        <tr>
          <th scope="col">Nome</th>
          <th scope="col">Email</th>
          <th scope="col">Acesso</th>
          <th scope="col">Ações</th>
        </tr>
        </thead>
        <tbody>
        <% @users.each do |user| %>
          <tr class="table-active">
            <%= render user %>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<div id="modal-operator" class="modal hide fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog " role="document">
    <div class="modal-content"></div>
  </div>
</div>